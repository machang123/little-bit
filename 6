#include<stdio.h>
#include<stdlib.h>
int main()
{ 
	int arr[10] = { 0 };
	int i = 0;
	int max = 0;
	printf("请输入十个整数：");
	for (i = 0; i < 10; i++)
	{ 
		scanf_s("%d", &arr[i]);
		max = arr[0];
	} 
	for (i = 0; i < 10; i++) 
	{ 
		if (arr[i]>max) 
		{
			max = arr[i];
		} 
	} 
	printf("十个数中的最大值为：%d\n", max);
	system("pause");
	return 0; 
}
